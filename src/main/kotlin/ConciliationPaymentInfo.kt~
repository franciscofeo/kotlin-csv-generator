data class CnabPaymentInfo(
    val orderId: String,
    val paymentId: String,
    val paymentValue: Int,
    val paymentMethodFee: Int,
    val netAmount: Int,
    val paymentDate: String,
    val currency: String,
    val paymentMethod: String,
    val ddc: String,
    val paymentStatus: String,
    val settlementId: String,
    val settlementDate: String,
    val settlementValue: Int,
    val settlementStatus: String,
    val settlementBeneficiary: String
) {
    companion object {
        val HEADERS = listOf(
            "order_ID" to CnabPaymentInfo::orderId,
            "Payment ID" to CnabPaymentInfo::paymentId,
            "Payment value" to CnabPaymentInfo::paymentValue,
            "Payment method fee" to CnabPaymentInfo::paymentMethodFee,
            "Net amount" to CnabPaymentInfo::netAmount,
            "Payment date" to CnabPaymentInfo::paymentDate,
            "Currency" to CnabPaymentInfo::currency,
            "Payment method" to CnabPaymentInfo::paymentMethod,
            "DDC" to CnabPaymentInfo::ddc,
            "Payment Status" to CnabPaymentInfo::paymentStatus,
            "Settlement ID" to CnabPaymentInfo::settlementId,
            "Settlement date" to CnabPaymentInfo::settlementDate,
            "Settlement value" to CnabPaymentInfo::settlementValue,
            "Settlement status" to CnabPaymentInfo::settlementStatus,
            "Settlement Beneficiary" to CnabPaymentInfo::settlementBeneficiary
        )
    }
} 